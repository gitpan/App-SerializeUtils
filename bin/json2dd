#!/usr/bin/perl

package json2dd; # make PodWeaver happy

use 5.010;
use strict;
use warnings;

use Data::Dump;
use JSON;

our $VERSION = '0.03'; # VERSION

undef $/;
dd(from_json(scalar <>));

1;
#ABSTRACT: Convert JSON to Data::Dump serialization

__END__

=pod

=head1 NAME

json2dd - Convert JSON to Data::Dump serialization

=head1 VERSION

version 0.03

