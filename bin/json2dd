#!/usr/bin/perl

use 5.010;
use strict;
use warnings;

use Data::Dump;
use JSON;

our $VERSION = '0.06'; # VERSION

undef $/;
dd(JSON->new->allow_nonref->decode(~~<>));

1;
#ABSTRACT: Convert JSON to Data::Dump serialization
#PODNAME: json2dd

__END__

=pod

=encoding utf-8

=head1 NAME

json2dd - Convert JSON to Data::Dump serialization

