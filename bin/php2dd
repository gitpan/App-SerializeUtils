#!/usr/bin/perl

package php2dd; # make PodWeaver happy

use 5.010;
use strict;
use warnings;

use Data::Dump;
use PHP::Serialization qw(unserialize);

our $VERSION = '0.04'; # VERSION

undef $/;
dd(unserialize(scalar <>));

1;
#ABSTRACT: Convert PHP to Data::Dump serialization

__END__

=pod

=head1 NAME

php2dd - Convert PHP to Data::Dump serialization

=head1 VERSION

version 0.04

