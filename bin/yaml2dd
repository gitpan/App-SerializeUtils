#!/usr/bin/perl

package yaml2dd; # make PodWeaver happy

use 5.010;
use strict;
use warnings;

use YAML::Syck; $YAML::Syck::ImplicitTyping = 1;
use Data::Dump;

our $VERSION = '0.04'; # VERSION

undef $/;
dd(Load(scalar <>));

1;
#ABSTRACT: Convert YAML to Data::Dump serialization

__END__

=pod

=head1 NAME

yaml2dd - Convert YAML to Data::Dump serialization

=head1 VERSION

version 0.04

