#!/usr/bin/perl

package dd2json; # make PodWeaver happy

use 5.010;
use strict;
use warnings;

use JSON;

our $VERSION = '0.03'; # VERSION

undef $/;
print to_json(eval(scalar <>));

1;
#ABSTRACT: Convert Data::Dump to JSON serialization

__END__

=pod

=head1 NAME

dd2json - Convert Data::Dump to JSON serialization

