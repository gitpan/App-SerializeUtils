#!/usr/bin/perl

package php2yaml; # make PodWeaver happy

use 5.010;
use strict;
use warnings;

use YAML::Syck; $YAML::Syck::ImplicitTyping = 1;
use PHP::Serialization qw(unserialize);

our $VERSION = '0.03'; # VERSION

undef $/;
print Dump(unserialize(scalar <>));

1;
#ABSTRACT: Convert PHP to YAML serialization

__END__

=pod

=head1 NAME

php2yaml - Convert PHP to YAML serialization

=head1 VERSION

version 0.03

