#!/usr/bin/perl

package dd2yaml; # make PodWeaver happy

use 5.010;
use strict;
use warnings;

use YAML::Syck; $YAML::Syck::ImplicitTyping = 1;

our $VERSION = '0.04'; # VERSION

undef $/;
print Dump(eval(scalar <>));

1;
#ABSTRACT: Convert Data::Dump to YAML serialization

__END__

=pod

