#!/usr/bin/perl

package json2yaml; # make PodWeaver happy

use 5.010;
use strict;
use warnings;

use YAML::Syck; $YAML::Syck::ImplicitTyping = 1;
use JSON;

our $VERSION = '0.03'; # VERSION

undef $/;
print Dump(from_json(scalar <>));

1;
#ABSTRACT: Convert JSON to YAML serialization

__END__

=pod

=head1 NAME

json2yaml - Convert JSON to YAML serialization

=head1 VERSION

version 0.03

